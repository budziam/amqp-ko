{
  "name": "amqp-ko",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p tsconfig.json -t stylish",
    "lint:fix": "yarn lint --fix",
    "pretty": "prettier '{src,test}/**/*.ts' --check",
    "pretty:fix": "yarn pretty --write",
    "postinstall": "tsc",
    "test": "jest --reporters=default"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/node": "^12.6.2",
    "amqplib": "^0.5.3",
    "moment": "^2.24.0",
    "snake-case": "^2.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
