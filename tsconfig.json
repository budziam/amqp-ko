{
  "compilerOptions": {
    "sourceMap": false,
    "target": "es5",
    "lib": ["es2015","esnext"],
    "moduleResolution": "node",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "downlevelIteration": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*"
  ]
}
